if not game:GetService("CoreGui"):FindFirstChild("ACA") then
local ACA = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local PenumbraShadow = Instance.new("ImageLabel")
local _189707 = Instance.new("Frame")
local RigM = Instance.new("TextButton")
local UIGridLayout = Instance.new("UIGridLayout")
local NoFall = Instance.new("TextButton")
ACA.Name = "ACA"
ACA.Parent = game:GetService("CoreGui")
ACA.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
