local function BANE_fake_script()
	local script = Instance.new('LocalScript', Title)

	script.Parent.Text = "ACA Hub - "..game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
end
coroutine.wrap(BANE_fake_script)()
local function WTBVT_fake_script()
	local script = Instance.new('LocalScript', RigM)

	script.Parent.MouseButton1Click:Connect(function()
		if game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("MainGui"):WaitForChild("MapVotePage").Visible == true then
			game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("MainGui"):WaitForChild("MapVotePage").Visible = false
		else
			game:GetService("Players").LocalPlayer.PlayerGui:WaitForChild("MainGui"):WaitForChild("MapVotePage").Visible = true
		end
	end)
end
coroutine.wrap(WTBVT_fake_script)()
local function HTNIVV_fake_script()
	local script = Instance.new('LocalScript', NoFall)

	script.Parent.MouseButton1Click:Connect(function()
		if game:GetService("Players").LocalPlayer.Character:FindFirstChild("FallDamageScript") then
			game:GetService("Players").LocalPlayer.Character:FindFirstChild("FallDamageScript"):Destroy()
			end
	end)
end
coroutine.wrap(HTNIVV_fake_script)()
local function GYYV_fake_script()
	local script = Instance.new('LocalScript', MainFrame)

	script.Parent.Title.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1  then
			local lastMousePos = Vector2.new(game.Players.LocalPlayer:GetMouse().X,game.Players.LocalPlayer:GetMouse().Y)
			while game:GetService("UserInputService"):IsMouseButtonPressed(Enum.UserInputType.MouseButton1 ) do
				local delta = Vector2.new(game.Players.LocalPlayer:GetMouse().X,game.Players.LocalPlayer:GetMouse().Y) - lastMousePos
				script.Parent.Parent.MainFrame.Position = script.Parent.Parent.MainFrame.Position + UDim2.new(0,delta.X,0,delta.Y)
				lastMousePos = Vector2.new(game.Players.LocalPlayer:GetMouse().X,game.Players.LocalPlayer:GetMouse().Y)
	  		 	wait()
	 		end
	 	end
	end)
end
coroutine.wrap(GYYV_fake_script)()
local function NBMFC_fake_script()
	local script = Instance.new('LocalScript', ACA)

	if script.Parent.MainFrame:FindFirstChild(game.PlaceId) then
		script.Parent.MainFrame:FindFirstChild(game.PlaceId).Visible = true
	else
		warn("------------------------------\n⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇⬇\nTHIS IS A INVALID GAME, PLEASE\nCHECK THE AVAILABLE GAMES IN\nTHE DISCORD: acahub.repl.co/\n⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆⬆\n------------------------------")
		script.Parent:Destroy()
	end
end
coroutine.wrap(NBMFC_fake_script)()
end
